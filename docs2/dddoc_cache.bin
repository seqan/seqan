p11687
aS'for (unsigned i = 0; i < length(lmStore.matchStore); ++i)'
p11688
aS'    std::cout << lmStore.sequenceNameStore[lmStore.matchStore[i].queryId] << "\\t"'
p11689
aS'              << lmStore.matchStore[i].queryBeginPos << "\\t"'
p11690
aS'              << lmStore.matchStore[i].queryEndPos << "\\t"'
p11691
aS'              << lmStore.sequenceNameStore[lmStore.matchStore[i].subjectId] << "\\t"'
p11692
aS'              << lmStore.matchStore[i].subjectBeginPos << "\\t"'
p11693
aS'              << lmStore.matchStore[i].subjectEndPos << "\\n";'
