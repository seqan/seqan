aS'..example.text: Calculate the alignment free sequence similarity o two masked DNA sequences.'
p19826
aS'..example.code:'
p19827
aS'using namespace seqan;'
p19828
aS"// Masked sequence, we do not want to count words overlapping 'N'"
p19829
aS'Dna5String sequenceDna5 ='
p19830
aS'    "TAGGTTTTCCGAAAAGGTAGCAACTTTACGTGATCAAACCTCTGACGGGGTTTTCCCCGTCGAAATTGGGTG"'
p19831
aS'    "TTTCTTGTCTTGTTCTCACTTGGGGCATCTCCGTCAAGCCAAGAAAGTGCTCCCTGGATTCTGTTGCTAACG"'
p19832
aS'    "AGTCTCCTCTGCATTCCTGCTTGACTGATTGGGCGGACGGGGTGTCCACCTGACGCTGAGTATCGCCGTCAC"'
p19833
aS'    "GGTGCCACATGTCTTATCTATTCAGGGATCAGAATTTATTCAGGAAATCAGGAGATGCTACACTTGGGTTAT"'
p19834
aS'    "CGAAGCTCCTTCCAAGGCGTAGCAAGGGCGACTGAGCGCGTAAGCTCTAGATCTCCTCGTGTTGCAACTACA"'
p19835
aS'    "CGCGCGGGTCACTCGAAACACATAGTATGAACTTAACGACTGCTCGTACTGAACAATGCTGAGGCAGAAGAT"'
p19836
aS'    "CGCAGACCAGGCATCCCACTGCTTGAAAAAACTATNNNNCTACCCGCCTTTTTATTATCTCATCAGATCAAG";'
p19837
ag21
aS'String<unsigned> kmerCounts;'
p19838
aS'unsigned k = 2;  // Count all 2-mers'
p19839
aS'countKmers(kmerCounts, sequenceDna5, k);'
p19840
ag21
aS'for(unsigned i = 0; i<16; ++i)       // Print the 2-mer counts'
p19841
aS'    std::cout<<kmerCounts[i]<<"\\n";  // 34 times AA; 30 times AC; 28 times AG; ...'
p19842
ag21
ag21
aS'String<double> nucleotideFrequencies;  // Defines a Bernoulli model for DNA sequences.'
p19843
aS'// Count all 2-mers and save the nucleotide frequencies'
p19844
aS'countKmers(kmerCounts, nucleotideFrequencies, sequenceDna5, k);'
p19845
ag21
